{
    "name": "paqtcom/assert",
    "description": "Assertions to validate method input/output with nice error messages.",
    "authors": [
        {
            "name": "Ian Scheele",
            "email": "ian.scheele@paqt.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PAQT\\Assert\\": "src"
        }
